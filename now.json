{
	"version": 2,
	"name": "omdesign",
	"env": {
		"NODE_ENV": "production"
	},
	"alias": [
		"omdesign.now.sh",
		"www.omdesign.cz"
	],
	"builds": [
		{
			"src": "next.config.js",
			"use": "@now/next"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"status": 301,
			"headers": {
				"Location": "https://omdesign.cz/$1"
			}
		}
	]
}